#include<iostream>
using namespace std;

void movLeft(string* b)
{
}

void movRight(string* b)
{
}

void movFront(string* b)
{
}

void movBack(string* b)
{
}

int main()
{
	string box[4][4];
	string Cbox[1][1];
	string Pbox[1][1];

	for(int i=0; i<4; i++)
	{
		for(int j=0; j<4; j++)
		{
			box[i][j]="";
		}
	}
	box[0][0] = "bot";
	Cbox[0][0] = box[0][0];
	
	while(Cbox != "gold")
	{
		if(Cbox == "strench" || Cbox == "breeze")
		{
			Cbox = Pbox;
			movBack(&Cbox[0][0]);
		}
		else
		{
			movLeft(&Cbox[0][0]);
		}
	
	}
	
	
	return 0;
}
